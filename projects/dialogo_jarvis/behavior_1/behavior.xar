                        <script language="4">
                          <content>
                            <![CDATA[#comportamento hello com luzes led nos olhos
class MyClass(GeneratedClass):
  def __init__(self):
    GeneratedClass.__init__(self, False)

  #metodo p carregar dados da memoria
  def onLoad(self):
    self.bIsRunning = False; #metodo p ele iniciar o moovimento
    self.leds = ALProxy("ALLeds") #metodo dos leds

  def onUnload(self):
    self.onInput_onStop(); #encerra a execucao do loop

  def onInput_onStart(self):
    #self.logger.info( self.getName() + ": start - begin" );

    if( self.bIsRunning ):
      #print( self.getName() + ": already started => nothing" );
      return;

    #inicia movimento e comportamento dos leds
    self.bIsRunning = True;
    self.bMustStop = False;

    rDuration = 0.2; #duracao
    self.leds.post.fadeRGB( "FaceLedsTop", 0xff00ff, rDuration );
    self.leds.post.fadeRGB( "FaceLedsInternal", 0xff00ff, rDuration );
    self.leds.post.fadeRGB( "FaceLedsBottom", 0xff00ff, rDuration );
    self.leds.fadeRGB( "FaceLedsExternal", 0xff00ff, rDuration );

    while( not self.bMustStop ):
      rTime = 0.1;
      self.leds.post.fadeRGB( "FaceLedsTop", 0xffffff, rTime );
      self.leds.fadeRGB( "FaceLedsBottom", 0xffffff, rTime );
      if( self.bMustStop ):
        break;
      rTime = 0.3
      self.leds.post.fadeRGB( "FaceLedsTop", 0xff00ff, rTime );
      self.leds.fadeRGB( "FaceLedsBottom", 0xff00ff, rTime );


    #encerrando
    self.bIsRunning = False;
    self.onStopped();

  def onInput_onStop(self):
    self.bMustStop = True; #encerra execucao loop]]>
