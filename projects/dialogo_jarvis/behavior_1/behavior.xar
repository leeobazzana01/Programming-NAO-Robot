                            <![CDATA[import requests
import uuid

# File paths
audio_file_path = "/home/nao/recordings/microphones/recording.ogg"
photo_file_path = "/home/nao/recordings/cameras/image.jpg"  # Update with actual path
api_url = ""
token = "Bazana67"

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.atts = ALProxy('ALAnimatedSpeech')
        self.memory = ALProxy("ALMemory")

    def onLoad(self):
        pass  # Initialization logic if needed

    def onUnload(self):
        pass  # Cleanup logic if needed

    def onInput_onStart(self):
        self.logger.debug("Starting process")

        # Retrieve data from robot memory
        result = self.memory.getData("chat-id")
        serverIP = self.memory.getData("server-ip")
        persona = self.memory.getData("persona")
        responseLength = self.memory.getData("responseLength")
        aiVersion = self.memory.getData("ai-version")

        # Validate server IP
        if not serverIP:
            self.logger.debug("Server IP not found, aborting.")
            return

        # Construct the API URL
        api_url = "http://"+serverIP+"/speech/id/"+result+"/culture/en-GB/raw/false/persona/"+persona+"/responselength/"+responseLength+"/ai-version/"+aiVersion
        self.logger.debug("URL = "+api_url)

        # Read the audio and photo files
        files = {}

        try:
            with open(photo_file_path, 'rb') as photo_file:
                files['photo'] = ('image.jpg', photo_file.read(), 'image/jpeg')
        except:
            self.logger.debug("Photo file not found, skipping.")

        try:
            with open(audio_file_path, 'rb') as audio_file:
                files['audio'] = ('recording.ogg', audio_file.read(), 'audio/ogg')
        except:
            self.logger.debug("Audio file not found, skipping.")


        # Ensure at least one file is being sent
        if not files:
            self.logger.debug("No files found to upload, aborting.")
            return

        # API Request
        headers = {'Authorization': token}
        # Make a POST request to the web API with the file data
        response = requests.post(api_url,headers=headers, files=files)

        # Check if the request was successful
        if response.status_code == 200:
            # log response, helpfull if logview is open.
            self.logger.debug(response.content)

            # User is stopping the chat session.
            if response.content == "STOP":
                self.stopOutput("no")
            # An invalid respons - exit app.
            elif response.content == "":
                self.killOutput("kill app")
            # skip this response, could be a duplicate call.
            elif response.content != "skip":
                #self.atts.say(response.content)
                self.atts.say(response.content, {'bodyLanguageMode': 'contextual',           'disableArmsAnimations': False, 'disableBodyAnimations': False, 'disableHeadAnimations': False, 'useArmAndHandMovement': True})

            self.isRunning = False
            self.onStopped()
        else:
            self.logger.debug("Failed to upload file. Status code:",response.status_code )
    pass

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
